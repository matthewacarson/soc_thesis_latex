digraph {
	size="50,29.98878923766816!" splines=ortho
	worker [label="Construction
Worker" pos="5.5,0!" shape=note]
	worker_register [label="Registers on
Union
Out-of-work List" pos="5.5,3!" shape=oval]
	union_hirehall [label="Union Hiring Hall" pos="3,3!" shape=note]
	employer [label=Employer pos="0,0!" shape=note]
	employer_request [label="Employer
Requests
Worker" pos="3,1.5!" shape=oval]
	employer_layoff [label="Employer
Lays-off
Worker" pos="3,0!" shape=oval]
	union_dispatch [label="Union Dispatches
Worker (same pay
and benefits as
previous employer)" pos="0,3!" shape=oval]
	worker_register -> union_hirehall
	employer -> employer_request
	employer_request -> union_hirehall
	union_hirehall -> union_dispatch
	union_dispatch -> employer
	employer -> employer_layoff
	employer_layoff -> worker
	worker -> worker_register
}
