@article{finkMemoirSeligPerlman1991,
  title = {A Memoir of {{Selig Perlman}} and His Life at the {{University}} of {{Wisconsin}}: {{Based}} on an Interview of {{Mark Perlman}} Conducted and Edited by {{Leon Fink}}},
  shorttitle = {A Memoir of {{Selig Perlman}} and His Life at the {{University}} of {{Wisconsin}}},
  author = {Fink, Leon},
  date = {1991-09-01},
  journaltitle = {Labor History},
  volume = {32},
  number = {4},
  pages = {503--525},
  publisher = {Routledge},
  issn = {0023-656X},
  doi = {10.1080/00236569100890301},
  url = {https://doi.org/10.1080/00236569100890301},
  urldate = {2024-05-27},
  abstract = {While beginning research on the labor reformers of the Progressive Era, I was struck by the paucity of biographical information about Selig Perlman, one of the seminal members of the “Wisconsin School” of labor historians. In seeking to supplement the existing record, I recorded an interview on January 29, 1989, with Selig Perlman's son, Mark Perlman, University Professor of Economics at the University of Pittsburgh. As edited below, Mark Perlman's testimony, I believe, offers a most valuable perspective on the life of his father, the surrounding circle of teachers and students at the University of Wisconsin, and the generational as well as intellectual transitions within the field of labor scholarship through the early 1930s. Mark Perlman's perspective is conditioned both by personal experience and by his own professional training, initially in the same fields of labor economics and labor history pioneered by his father and his father's teacher, John R. Commons. At the personal level, the son spent many hours alone with his father (Mark's mother, Eva Shaber, died in 1930 when Mark was six, whence his father re-married the mother's younger sister, Fannie); from age 13 he calculates that he listened to his father talk about “the evolution of his ideas … his problems of getting along with people,” etc. for “the better part of four or five hours a week.” This closeness, Mark allows, was less a “comradeship” than “a European father with a very devoted, admiring son.” From the time the son was in junior high school, in fact, the father had convinced him to follow in his own scholarly footsteps. Mark took his BA and MA in economics at the University of Wisconsin and even briefly served as his father's graduate assistant. In 1950 he received his PhD under Carter Goodrich at Columbia University. His dissertation was published in 1954 as Judges in Industry: A Study in Labour Arbitration in Australia. During appointments at the University of Hawaii, Cornell, and Johns Hopkins, Mark Perlman pursued a labor-centered research agenda represented by Labor Union Theories in America: Background and Development (1958), The Machinists: A New Study in American Trade Unionism (1961), and Democracy in the IAM (1962). Subsequently, in a distinguished career at the University of Pittsburgh, Mark Perlman's research emphasis shifted to health and population economics, even as he maintained a continuing interest in the history of economics. He created in 1968 the American Economic Association's pace-setting Journal of Economic Literature. Incidentally, it may interest readers that in addition to labor scholars and economists, the Perlman line produced Judith Martin, Selig's niece and Mark's cousin, author of the famous, and ongoing, “Miss Manners” etiquette column.},
  file = {C:\Users\madou\Zotero\storage\SWYJ42KE\Fink - 1991 - A memoir of Selig Perlman and his life at the University of Wisconsin Based on an interview of Mark.pdf}
}

