@article{levinePrehireAgreementsDoublebreasting1988,
  title = {Pre-Hire {{Agreements}} and "{{Doublebreasting}}" in the {{Construction Industry}}: {{Prospects}} for {{Legislative Change}}},
  shorttitle = {Pre-Hire {{Agreements}} and "{{Doublebreasting}}" in the {{Construction Industry}}},
  author = {Levine, Marvin J.},
  year = {1988},
  month = apr,
  journal = {Labor Law Journal},
  volume = {39},
  number = {4},
  pages = {247--253},
  issn = {00236586},
  urldate = {2023-06-23},
  abstract = {This article focuses on pre-hire agreements and doublebreasting in the construction industry of the United States. The construction industry plays a major economic role in the United States. In 1986, construction activity contributed \$340.6 billion to the gross national product, employing nearly five million persons. The pre-hire agreements enabled the employers to predict and estimate labor costs and requirements and to ensure a steady supply of skilled workers. Also, since the employer relied on the union for its employees, the union would in fact represent a majority of employees. The concept of doublebreasting refers to a creation of two distinct companies, one of which is a party to a collective bargaining agreement and one that is not. In doing this, a company is able to participate in a wider variety of projects and remain competitive in the industry. There are a few ways in which the unions have challenged doublebreasting. The most common way is when a union representing the employees of the unionized part of the double-breasted company seeks to force the nonunion "open" shop employer to recognize the collective bargaining agreement.},
  keywords = {Collective bargaining,Construction industry,Employees,Gross national product,Labor costs,Labor unions,United States},
  annotation = {Prehire agreement legislation background. Publisher: CCH Incorporated},
  file = {C:\Users\madou\Zotero\storage\GYZPEFVL\Levine - 1988 - Pre-hire Agreements and Doublebreasting in the C.pdf}
}

