@thesis{koscielskiDividedLoyaltiesAmerican,
  type = {phdthesis},
  title = {Divided Loyalties: {{American}} Unions and the {{Vietnam War}}},
  shorttitle = {Divided Loyalties},
  author = {Koscielski, Frank F.},
  institution = {Wayne State University},
  location = {United States -- Michigan},
  url = {https://www.proquest.com/docview/304376018/abstract/4FFEF44692EE4176PQ/1},
  urldate = {2024-06-03},
  abstract = {The dominant historiographic view and the popular stereotype hold that American labor unequivocally supported the war in Vietnam from the Tonkin Gulf Resolution in 1964 to the Paris Peace Agreement of 1973. This image was created mainly as a result of AFL-CIO leader George Meany's support of both the Johnson and Nixon administrations on the war, and the attacks on antiwar protesters in New York in 1970 by unionized construction workers, the so-called hardhats. Another school of thought contends that after some initial hesitation, workers carried on a vigorous antiwar campaign joining hands with student and New Left protesters. Both these views are far too simplistic. Utilizing the public record, the Reuther Archives, and oral histories and interviews, I examined three aspects of organized labor: the AFL-CIO and George Meany, the UAW and Walter Reuther, and Local 600, the Ford Rouge Plant in Dearborn Michigan. I argue that labor's response was much more complicated and nuanced than most experts have contended; working people held multiple, diverse and often divergent views on the war that changed over time. My examination of portions of the world of labor from top union leadership through factory workers in different trades and portions reveals a multiplicity of responses that reflected a host of factors within and outside the world of work. The labor movement simply mirrored American society. Workers, however, did not generally participate in mass antiwar protests. Many working people hated the war and were aware that they were paying its price in dollars and blood. But they also hated the antiwar movement they had come to know primarily through the media. As the movement became more mainstream, especially after 1970, more workers did raise their voices in protest. Thus, both the stereotype of unequivocal labor support for the war and the concept of the average worker as antiwar crusader should be put to rest.},
  isbn = {9780591664775},
  langid = {english},
  pagetotal = {238},
  keywords = {AFL CIO,foreign relations,labor,Social sciences,UAW},
  file = {C:\Users\madou\Zotero\storage\KGG6JXA2\Koscielski - Divided loyalties American unions and the Vietnam War.pdf}
}

