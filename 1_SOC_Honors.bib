@article{trongoneNEWCOLLECTIVEBARGAIN2022,
	title = {A {NEW} {COLLECTIVE} {BARGAIN}? A {MULTICASE} {COMPARISON} {OF} U.S. {LABOR} {UNION} {STRATEGY}*},
